{
	"name": "@plentymarkets/terra-components",
	"version": "4.5.0",
	"homepage": "https://github.com/plentymarkets/terra-components#readme",
	"author": "plentymarkets",
	"description": "plentymarkets terra components",
	"repository": {
		"type": "git",
		"url": "git+https://plentymarkets@github.com/plentymarkets/terra-components.git"
	},
	"keywords": [
		"angular2",
		"plentymarkets",
		"terra",
		"components"
	],
	"license": "MIT",
	"scripts": {
		"build": "rimraf dist && gulp build",
		"build-lib": "ng-packagr -p src/lib/package.json",
		"build-doc": "gulp build-doc",
		"build-test": "rimraf dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
		"build:lint-rules": "tsc -p lintRules/tsconfig.lint.json",
		"release": "gulp npm-publish",
		"start": "ng serve",
		"gulp": "gulp",
		"test": "ng test",
		"test-once": "ng test --watch=false --browsers=Chrome,Firefox,Safari",
		"test-headless": "ng test --watch=false --browsers=ChromeHeadless,FirefoxHeadless",
		"lint": "tslint -p tsconfig.json -c tslint.json -e 'src/**/*.d.ts' -e 'src/**/*.spec.ts' -e 'src/lib/environments/**' -e 'src/test.ts' 'src/**/*.ts'",
		"reinstall": "rm -rf node_modules/ && npm i"
	},
	"dependencies": {
		"bootstrap": "^4.3.1",
		"flatted": "^2.0.0",
		"htmlhint": "~0.10.3",
		"iban": "^0.0.12",
		"interactjs": "1.3.4",
		"jquery": "^3.4.1",
		"lodash": "^4.17.11",
		"moment": "^2.24.0",
		"mydatepicker": "^2.6.6",
		"ng2-ace-editor": "^0.3.9",
		"ngx-bootstrap": "^4.3.0",
		"ngx-quill": "^5.2.0",
		"path": "^0.12.7",
		"popper.js": "^1.15.0",
		"quill": "^1.3.6",
		"reflect-metadata": "^0.1.13",
		"tippy.js": "^4.3.4",
		"ts-keycode-enum": "^1.0.6"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.13.0",
		"@angular-devkit/build-ng-packagr": "~0.13.0",
		"@angular/animations": "~7.2.15",
		"@angular/cdk": "^7.3.7",
		"@angular/cli": "~7.3.9",
		"@angular/common": "~7.2.15",
		"@angular/compiler": "~7.2.15",
		"@angular/compiler-cli": "~7.2.0",
		"@angular/core": "~7.2.15",
		"@angular/forms": "~7.2.15",
		"@angular/http": "~7.2.15",
		"@angular/language-service": "~7.2.0",
		"@angular/material": "^7.3.7",
		"@angular/platform-browser": "~7.2.15",
		"@angular/platform-browser-dynamic": "~7.2.15",
		"@angular/router": "~7.2.15",
		"@interactjs/types": "^1.6.2",
		"@types/htmlhint": "^0.9.1",
		"@types/jasmine": "~2.8.8",
		"@types/jasminewd2": "~2.0.3",
		"@types/jquery": "^3.3.29",
		"@types/node": "~8.9.4",
		"angular-l10n": "7.2.0",
		"ckeditor4-angular": "^1.0.0-beta",
		"codelyzer": "~5.1.2",
		"dgeni": "^0.4.12",
		"dgeni-packages": "^0.27.3",
		"jasmine-core": "~2.99.1",
		"jasmine-spec-reporter": "~4.2.1",
		"karma": "~4.0.0",
		"karma-chrome-launcher": "~2.2.0",
		"karma-coverage-istanbul-reporter": "~2.0.1",
		"karma-firefox-launcher": "^1.2.0",
		"karma-jasmine": "~1.1.2",
		"karma-jasmine-html-reporter": "^0.2.2",
		"karma-safari-launcher": "^1.0.0",
		"ng-packagr": "^4.2.0",
		"protractor": "~5.4.0",
		"rxjs": "~6.3.3",
		"scss-bundle": "^3.0.1",
		"ts-node": "~7.0.0",
		"tsickle": ">=0.34.0",
		"tslib": "^1.9.0",
		"tslint": "^5.17.0",
		"tslint-eslint-rules": "^5.4.0",
		"typescript": "~3.2.2",
		"zone.js": "~0.8.26"
	}
}
