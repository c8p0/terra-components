	"license": "MIT",
	"scripts": {
		"build": "rimraf dist && gulp build",
		"build-doc": "gulp build-doc",
		"build-test": "rimraf dist && webpack --config config/webpack.prod.js  --progress --profile --bail",
		"release": "gulp npm-publish",
		"start": "webpack-dev-server --progress --watch --profile --open --config config/webpack.dev.js",
		"gulp": "gulp",
		"test": "karma start",
		"test-once": "karma start --single-run --browsers Chrome,Firefox,Safari",
		"test-headless": "karma start --single-run --browsers ChromeHeadless,FirefoxHeadless",
		"lint": "NODE_OPTIONS='-r ts-node/register' tslint -p tsconfig.json -c tslint.json -e 'src/**/*.d.ts' -e 'src/**/*.spec.ts' 'src/**/*.ts'",
		"reinstall": "rm -rf node_modules/ && npm i"
	},
